<h1>Users</h1>

<%= link_to "Create new user", new_user_path %>

<table>
  <tr>
    <th>Email</th>
    <th></th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= link_to "Show", user_path(user) %> | <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Are you sure?"} %></td>
  </tr>
  <% end %>
</table>