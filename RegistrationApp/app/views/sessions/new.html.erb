<h1>Login</h1>

<%= link_to "Register", new_user_path %>

<%= form_for :session, url: login_path do |f| %>
  <div>  
    <%= f.label :email, "Email or username" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Log in" %>
  </div>
<% end %>